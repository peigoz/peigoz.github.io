{
  "envId": "peigo-1605fd",
  "version": "2.0",
  "$schema": "https://framework-1258016615.tcloudbaseapp.com/schema/latest.json",
  "functionRoot": "./functions",
  "functions": [],
  "region": "ap-shanghai",
  "framework": {
    "name": "PeigoBlog",
    "plugins": {
      "vuepress": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "buildCommand": "git add . && git commit -m '' && npm run docs:build",
          "outputPath": "docs/.vuepress/dist",
          "ignore": [
            ".git",
            ".github",
            "node_modules",
            "nginx.conf"
          ]
        }
      }
    }
  }
}
